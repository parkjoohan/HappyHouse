<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.happyhouse.model.mapper.CCTVMapper">

	<select id="getCCTVInfo" parameterType="map" resultType="CCTVDto">
		select * from seoul_cctv
    	where dong = #{dong}
      	order by gugun
       	limit #{start}, #{spp}
    </select>
<!-- 	<select id="getCCTVVideoInfo" resultType="video"> -->
<!-- 		 select * from cctvvideo -->
<!--     </select> -->
	<select id="getTotalCount" parameterType="map" resultType="int">
		select count(*)
		from seoul_cctv
		where dong = #{dong}
	</select>
</mapper>